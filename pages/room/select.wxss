.roomBg {
  background-color: white;
  padding: 10px;
  border-radius: 10px;
  margin: 10px;
  position: relative;
  height: 100px;
}

.roomList {
  padding-bottom: 70px;
}

.roomTitle {
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
  word-break: break-all;
  font-size: 16px;
  margin-right: 80px;
}

.btn_create {
  position: fixed;
  right: 10px;
  bottom: calc(env(safe-area-inset-bottom) + 10px);
  display: block;
  padding: 8px 16px;
  color: #fff;
  background-color: #666;
  border-radius: 5px;
  font-size: 14px;
  box-shadow: 0px 0px 20px rgba(0, 0, 0, 0.3);
}

.roomBottom {
  position: absolute;
  right: 10px;
  bottom: 10px;
  left: 10px;
  overflow: hidden;
}

.roomOnline {
  color: orangered;
}

.roomType {
  font-size: 16px;
  font-weight: bold;
  color: orangered;
  padding: 2px;
  margin-left: 5px;
}

.roomNotice {
  font-size: 12px;
  color: #AAAAAA;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
  display: inline-block;
}

.roomUser {
  margin-left: 50px;
  font-size: 14px;
  color: #333;
  margin-top: 10px;
  line-height: 22px;
}

.roomImage {
  width: 40px;
  height: 40px;
  border-radius: 10px;
  margin-top: 10px;
  position: absolute;
  background-color: #F5F5F5;
  border: 1px solid #F5F5F5;
}


.roomId {
  font-size: 12px;
  color: #666;
  background-color: #F5F5F5;
  padding: 1px 5px;
  border-radius: 3px;
}

.roomRight {
  position: absolute;
  right: 10px;
  top: 10px;
  vertical-align: middle;
}

.roomLock {
  color: orangered;
  font-size: 14px;
}

.tipImage {
  text-align: center;
  margin-top: 20%;

}

.tipImage .img {
  width: 100px;
  height: 100px;
}

.tipText {
  margin-top: 5%;
  font-size: 14px;
  color: #999;
  text-align: center;
}

.searchbox {
  margin: 10px;
  background-color: white;
  border: none;
  margin-bottom: 10px;
  border-radius: 10px;
}